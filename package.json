{
  "name": "cypressautomation",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "test": "cypress run",
    "headtest": "npm run test -- --headed",
    "chrometest": "npm run test -- --browser chrome",
    "recordDashboardtest": "npm run test -- --record --key 75e55150-89f3-4fa9-a2c8-4c3be79b478c --reporter mochawesome",
    "parallelDashboardtest": "npm run test -- --record --key 75e55150-89f3-4fa9-a2c8-4c3be79b478c --parallel",
    "alltest": "npm run test -- --spec cypress/e2e/*"
  },
  "cypress-cucumber-preprocessor": {
    "json": {
      "enabled": true,
      "output": "cypress/cucumberRepots/results.json"
    }
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "cypress": "^12.4.1",
    "cypress-iframe": "^1.0.1",
    "mochawesome": "^7.1.3",
    "multiple-cucumber-html-reporter": "^3.2.0",
    "neat-csv": "5.1.0"
  },
  "dependencies": {
    "@badeball/cypress-cucumber-preprocessor": "^15.1.4",
    "@cypress/browserify-preprocessor": "latest"
  }
}
